{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@stylistic/js"
    ],
    "rules": {
        "no-unused-vars": "warn",
        "@stylistic/js/semi": "warn",
        "no-empty": "warn"
    }
}
